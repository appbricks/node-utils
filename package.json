{
  "name": "@appbricks/utils",
  "version": "0.0.7",
  "description": "Common utilities",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --quiet --fix",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appbricks/node-utils.git"
  },
  "keywords": [
    "AppBricks"
  ],
  "author": "Mevan Samaratunga",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/appbricks/node-utils/issues"
  },
  "homepage": "https://github.com/appbricks/node-utils",
  "dependencies": {
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json",
    "lib/"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
