{
  "name": "@appbricks/utils",
  "version": "0.0.4",
  "description": "Common utilities",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --quiet --fix",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appbricks/node-utils.git"
  },
  "keywords": [
    "AppBricks"
  ],
  "author": "Mevan Samaratunga",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/appbricks/node-utils/issues"
  },
  "homepage": "https://github.com/appbricks/node-utils",
  "dependencies": {
    "react-redux": "^7.2.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.6",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
